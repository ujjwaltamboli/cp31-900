#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        unordered_map<long long,long long> freq;
        long long n,k;
        cin>>n>>k;
        string s;
        cin>>s;
        for (long long i=0;i<n;i++){//O(n)
            freq[s[i]]++;
        }
        long long count=0;
        for (auto it=freq.begin();it!=freq.end();it++){//O(n)
            if ((it->second%2)!=0){
                count++;
            }
        }
        if ((n-k)%2==0){
            if (k<count){
                cout<<"No"<<endl;
                continue;
            }
            k=k-count;
        }
        else{
            if (k<(count-1)){
                cout<<"No"<<endl;
                continue;
            }
            k=k-(count-1);
        }
        if (k%2==0){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }

        //T.C.:- O(n);
        //S.C.:- O(n);
    }
    return 0;
}
