#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        long long n;
        cin>>n;
        if (n%2==1 || (n<4)){
            cout<<-1<<endl;
            continue;
        }
        long long y=n/4;//MAX
        long long x=-1;//MIN
        if (n%6==0){
            x=n/6;
        }
        else if (n%6==2){
            x=n/6+1;
        }
        else if (n%6==4){
            x=n/6+1;
        }
        cout<<x<<" "<<y<<endl;
    }

    //T.C.:-O(1)
    //S.C.:-O(1)
    return 0;
}
