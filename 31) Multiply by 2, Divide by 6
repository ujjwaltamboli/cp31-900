#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int count3=0;
        int count2=0;
        while (n%3==0){
            n=n/3;
            count3++;
        }
        while (n%2==0){
            n=n/2;
            count2++;
        }

        if (n!=1 || count2>count3){
            cout<<-1<<endl;
            continue;
        }
        cout<<(count3-count2)+count3<<endl;

        //T.C.:- log(n);
        //S.C.:- O(1);
    }
    return 0;
}
