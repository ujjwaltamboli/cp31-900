#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int ans=INT_MAX;
        while (n--){
            int a;
            cin>>a;
            ans=ans&a;
        }
        cout<<ans<<endl;

        //T.C:- O(n);
        //S.C.:- O(1);
    }
}
