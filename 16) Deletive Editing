#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        unordered_map<int,int> freq;
        string s1;
        string s2;
        cin>>s1;
        cin>>s2;
        int n1=s1.length();
        int n2=s2.length();
        for (auto i:s2){
            freq[i]++;
        }
        string s="";
        for (int i=n1-1;i>=0;i--){//O(l1)
            if (freq[s1[i]]>0){//O(1)
                s+=s1[i];
            }
            if (freq[s1[i]]>0){
                freq[s1[i]]--;
            }
        }
        reverse(s.begin(),s.end());
        if (s==s2){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }
    }

    // t.c:- O(l1);
    //S.C.:- O(l1+l2);
    return 0;
}
