#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        string s1;
        cin>>s1;
        string s2;
        cin>>s2;
        unordered_map<char,int> freq;
        unordered_map<char,int> freq1;
        vector<bool> blacklist(s1.size(),false);
        for (int i=0;i<s2.length();i++){
            freq[s2[i]]++;
        }

        for (int i=0;i<s1.length();i++){
            if (freq[s1[i]]==0){
                blacklist[i]=true;
            }
            freq1[s1[i]]++;
        }

        for (int i=0;i<s1.length();i++){
            if (blacklist[i]){
                continue;
            }
            if (freq1[s1[i]]!=freq[s1[i]]){
                freq1[s1[i]]--;
                blacklist[i]=true;
            }
        }

        string s="";
        for (int i=0;i<s1.length();i++){
            if (blacklist[i]){
                continue;
            }
            s=s+s1[i];
        }

        if (s2==s){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }
        
    }
    return 0;
}
