#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        int maxi=0;
        map<int,int> freq;
        for (int i=0;i<n;i++){ //O(n)
            cin>>store[i];
            freq[store[i]]++;
            if (freq[store[i]]>maxi){
                maxi=freq[store[i]];
            }
            //O(logn)
        }
        //O(nlogn)
        int count=n-maxi;//2nd operation
        while (maxi<n){
            maxi=maxi*2;
            count++;
        }//log2(n)
        cout<<count<<endl;

        //T.C.:- O(nlogn);
        //S.C.:- O(n);
    }
    return 0;
}
