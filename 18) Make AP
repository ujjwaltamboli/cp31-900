#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int a,b,c;
        cin>>a>>b>>c;
        if ((b+(b-a))>=c && (b+(b-a))%c==0){
            cout<<"Yes"<<endl;
        }
        else if ((b-(c-b))>=a && (b-(c-b))%a==0){
            cout<<"Yes"<<endl;
        }
        else if (((a+c)/2.0)!=(a+c)/2){
            cout<<"No"<<endl;
        }
        else if (((a+c)/2)>b && ((a+c)/2)%b==0){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }
    }

    //T.C.:- O(1);
    //S.C.:- O(1);
    return 0;
}
