#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        long long n;
        cin>>n;
        vector<long long> store(n,0);
        for (long long i=0;i<n;i++){
            cin>>store[i];
        }
        for (long long i=0;i<n;i++){
            if (store[i]==1){
                store[i]++;
            }
        }
        for (long long i=1;i<n;i++){
            if (store[i]%store[i-1]==0){
                store[i]++;
            }
        }
        for (long long i=0;i<n;i++){
            cout<<store[i]<<" ";
        }
        cout<<endl;

        //T.C.:- O(n);
        //S.C.:- O(n)
    }
    return 0;
}
