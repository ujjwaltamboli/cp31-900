#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        long long int n;
        cin >> n;
        int mini=INT_MAX;

        vector<string> arr = {"50", "25", "75", "00"};

        for (auto i : arr)
        {
            int count = 0;
            long long int n1=n;
            int j=1;
            while (n1 != 0)
            {
                int digit=n1%10;
                count++;
                if (digit==(i[j]-'0')){
                    j--;
                    if (j==-1){
                        break;
                    }
                }
                n1 = n1 / 10;
            }
            mini=min(mini,count);
        }
        cout<<mini-2<<endl;

        //T.C.:- O(19*4)=O(1);
        //S.C.:- O(1);
    }
    return 0;
}
